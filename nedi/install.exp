#!/usr/bin/expect
set timeout 10
set user [lindex $argv 0]
set password [lindex $argv 1]
set dbhost [lindex $argv 5]

spawn /var/nedi/nedi.pl -i
expect "user:"
        send "$user\r"
        expect "pass:"
        send "$password\r";
expect "on:";
        send "$dbhost\r";
expect "done:"
#        interact